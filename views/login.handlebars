<div id="login-view">
    <img src="public/static/images/logo_icon.png" />
    <div id="form-view">
        <form >
            <input class="input-form" type="email" id="email-login" placeholder="email" name="email" /><br />
            <input class="input-form" type="password" id="pass-login" placeholder="password" name="password" /><br />
            <button type="button" onclick="login()">Submit</button>
        </form>
    
        <div class="err-div">{{{ERROR}}}</div>
    </div>
</div>